<%= form_for :session, url: sign_in_path do |f| %>
  <div class="field">
    <%= f.label :login %><br />
    <%= f.text_field :login, id:"login" ,autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :Senha %><br />
    <%= f.password_field :password, id:"senha", autocomplete: "off" %>
  </div>
  <div class="actions">
    <%= f.submit "Log in", id:"submit"%>
  </div>
  <p>New user? <%= link_to "Sign up now!", new_usuario_path %></p>
<% end %>