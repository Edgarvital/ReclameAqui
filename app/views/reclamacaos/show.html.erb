<p id="notice"><%= notice %></p>
<h1>Reclamação</h1>
<p>
  <strong>Titulo:</strong>
  <%= @reclamacao.titulo %>
</p>

<p>
  <strong>Texto:</strong>
  <%= @reclamacao.texto %>
</p>

<p>
  <strong>Categoria:</strong>
  <%= @reclamacao.categoria %>
</p>
<p>
  <strong>Empresa:</strong>
  <%= @reclamacao.empresa %>
</p>

<% if current_user != nil %>
  <%= link_to 'Editar', edit_usuario_reclamacao_path(@usuario, @reclamacao) %> |
<% end %>
<%= link_to 'Voltar', usuario_reclamacaos_path(@usuario) %>
